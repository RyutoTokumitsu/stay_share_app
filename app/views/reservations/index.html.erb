<h3>予約済み一覧</h3>
<table>
  <thead>
    <tr>
      <th>画像</th>
      <th>施設名</th>
      <th>紹介文</th>
      <th>合計料金</th>
      <th>チェックイン</th>
      <th>チェックアウト</th>
      <th>予約確定日</th>
    </tr>
  </thead>
  <tbody>
    <% @reservations.each do |reservation| %>
      <tr>
        <td><%= reservation.room.hotel_image %></td>
        <td><%= reservation.room.hotel_name %></td>
        <td><%= reservation.room.introduction %></td>
        <td><%= reservation.room.hotel_price %></td>
        <td><%= reservation.check_in.to_s(:datetime_jp) %></td>
        <td><%= reservation.check_out.to_s(:datetime_jp) %></td>
        <td><%= reservation.created_at.to_s(:reservetime_jp) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
